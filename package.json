{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "backend.git"
  },
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "build": "rm -rf dist && tsc --project tsconfig.compile.json && tsc-alias -p tsconfig.compile.json",
    "concat-prisma": "cat ./prisma/schemas/*.prisma > ./prisma/schema.prisma",
    "dev:tsed": "tsnd --inspect --ignore-watch node_modules --respawn --transpile-only --project tsconfig.json -r tsconfig-paths/register src/index.ts",
    "dev:express": "NODE_OPTIONS=--max-old-space-size=15360 tsnd --inspect --ignore-watch node_modules --respawn --transpile-only --project tsconfig.json -r tsconfig-paths/register src/index-express.ts",
    "dev": "cross-env NODE_ENV=development yarn dev:tsed & yarn dev:express",
    "prisma:generate": "yarn concat-prisma && yarn run prisma generate",
    "prisma:migrate": "yarn concat-prisma && yarn run prisma migrate dev --name init",
    "prisma:seed": "yarn run prisma db push --force-reset && NODE_OPTIONS=--max-old-space-size=15360 yarn run prisma db seed"
  },
  "_moduleAliases": {
    "~routes/*": "src/routes",
    "~config/*": "src/config",
    "~helpers/*": "src/helpers",
    "~generated/*": "prisma/generated",
    "~prisma/*": "prisma"
  },
  "dependencies": {
    "@apollo/server": "4.3.0",
    "@prisma/client": "4.8.1",
    "@tsed/adapters": "7.18.2",
    "@tsed/ajv": "7.18.2",
    "@tsed/cli-core": "4.2.7",
    "@tsed/common": "7.18.2",
    "@tsed/core": "7.18.2",
    "@tsed/di": "7.18.2",
    "@tsed/engines": "7.18.2",
    "@tsed/exceptions": "7.18.2",
    "@tsed/json-mapper": "7.18.2",
    "@tsed/logger": "6.3.3",
    "@tsed/logger-file": "6.3.3",
    "@tsed/passport": "7.18.2",
    "@tsed/platform-cache": "7.18.2",
    "@tsed/platform-exceptions": "7.18.2",
    "@tsed/platform-express": "7.18.2",
    "@tsed/platform-log-middleware": "7.18.2",
    "@tsed/platform-middlewares": "7.18.2",
    "@tsed/platform-params": "7.18.2",
    "@tsed/platform-response-filter": "7.18.2",
    "@tsed/platform-views": "7.18.2",
    "@tsed/prisma": "7.18.2",
    "@tsed/schema": "7.18.2",
    "@tsed/typegraphql": "7.18.2",
    "@types/graphql-fields": "1.3.4",
    "apollo-datasource": "^3.3.2",
    "apollo-datasource-rest": "^3.7.0",
    "apollo-server-core": "^3.11.1",
    "apollo-server-express": "3.11.1",
    "body-parser": "1.20.1",
    "bull": "^4.10.2",
    "class-validator": "0.14.0",
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "dotenv": "16.0.3",
    "dotenv-flow": "^3.2.0",
    "esbuild-node-tsc": "^2.0.5",
